from sage.all import *

N = 29106959165856738284067251061192224580743408600499253459380471046431384385596594611842183139277133507728406798047298
n = 59525075753336432903533101075050773102467306693252634736813555076374463333313
ct = 21650721713151311593696124114835254195816772581140927733455186035566980511215

for p_q in divisors(N): # p_q = p+q
    phi = n-p_q + 1
    d = pow(65537,-1,phi)
    m = int(pow(ct,d,n)).to_bytes(200, 'little')
    if b'ctf{' in m:
        print(m)
